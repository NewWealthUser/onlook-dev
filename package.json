{
    "name": "@onlook/repo",
    "version": "0.0.0",
    "description": "Onlook Monorepo",
    "homepage": "https://onlook.com",
    "license": "Apache-2.0",
    "private": true,
    "author": {
        "name": "Onlook",
        "email": "contact@onlook.com"
    },
    "workspaces": [
        "packages/*",
        "apps/web/client"
    ],
    "scripts": {
        "dev": "cd apps/web/client && npm run dev",
        "build": "cd apps/web/client && npm run build",
        "start": "cd apps/web/client && npm run start",
        "typecheck": "cd apps/web/client && npm run typecheck",
        "lint": "cd apps/web/client && npm run lint",
        "prepare": "husky",
        "clean": "git clean -xdf node_modules"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onlook-dev/onlook.git"
    },
    "bugs": {
        "url": "https://github.com/onlook-dev/onlook/issues"
    },
    "devDependencies": {
        "husky": "^9.1.6"
    }
}